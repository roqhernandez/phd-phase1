{
  "nodes": [
    {
      "id": "statistics",
      "metadata": {
        "type": "concept",
        "description": "Discipline of data collection, analysis, interpretation, and inference",
        "examples": [
          "Descriptive",
          "Inferential"
        ]
      }
    },
    {
      "id": "descriptive_statistics",
      "metadata": {
        "type": "concept",
        "description": "Summarize features of data",
        "examples": [
          "mean",
          "median",
          "variance"
        ]
      }
    },
    {
      "id": "inferential_statistics",
      "metadata": {
        "type": "concept",
        "description": "Generalize from samples to populations",
        "examples": [
          "hypothesis tests",
          "confidence intervals"
        ]
      }
    },
    {
      "id": "probability",
      "metadata": {
        "type": "concept",
        "description": "Mathematics of uncertainty",
        "examples": [
          "events",
          "random variables"
        ]
      }
    },
    {
      "id": "random_variable",
      "metadata": {
        "type": "concept",
        "description": "Variable determined by chance",
        "examples": [
          "X~Bernoulli(p)"
        ]
      }
    },
    {
      "id": "discrete_random_variable",
      "metadata": {
        "type": "concept",
        "description": "Takes countable values",
        "examples": [
          "Binomial",
          "Poisson"
        ]
      }
    },
    {
      "id": "continuous_random_variable",
      "metadata": {
        "type": "concept",
        "description": "Takes values on a continuum",
        "examples": [
          "Normal",
          "Exponential"
        ]
      }
    },
    {
      "id": "probability_distribution",
      "metadata": {
        "type": "concept",
        "description": "Mapping from outcomes to probabilities",
        "examples": [
          "PMF",
          "PDF",
          "CDF"
        ]
      }
    },
    {
      "id": "normal_distribution",
      "metadata": {
        "type": "concept",
        "description": "N(μ,σ²), bell-shaped",
        "examples": [
          "heights",
          "errors"
        ]
      }
    },
    {
      "id": "binomial_distribution",
      "metadata": {
        "type": "concept",
        "description": "#successes in n Bernoulli trials",
        "examples": [
          "quality control"
        ]
      }
    },
    {
      "id": "poisson_distribution",
      "metadata": {
        "type": "concept",
        "description": "Counts in fixed interval",
        "examples": [
          "arrivals per hour"
        ]
      }
    },
    {
      "id": "exponential_distribution",
      "metadata": {
        "type": "concept",
        "description": "Waiting times, memoryless",
        "examples": [
          "time between arrivals"
        ]
      }
    },
    {
      "id": "uniform_distribution",
      "metadata": {
        "type": "concept",
        "description": "All values equally likely on interval",
        "examples": [
          "U(0,1)"
        ]
      }
    },
    {
      "id": "mean",
      "metadata": {
        "type": "concept",
        "description": "Arithmetic average",
        "examples": [
          "x̄"
        ]
      }
    },
    {
      "id": "median",
      "metadata": {
        "type": "concept",
        "description": "Middle ordered value",
        "examples": [
          "50th percentile"
        ]
      }
    },
    {
      "id": "mode",
      "metadata": {
        "type": "concept",
        "description": "Most frequent value",
        "examples": [
          "bimodal data"
        ]
      }
    },
    {
      "id": "variance",
      "metadata": {
        "type": "concept",
        "description": "Expected squared deviation",
        "examples": [
          "σ²"
        ]
      }
    },
    {
      "id": "standard_deviation",
      "metadata": {
        "type": "concept",
        "description": "√variance",
        "examples": [
          "σ"
        ]
      }
    },
    {
      "id": "covariance",
      "metadata": {
        "type": "concept",
        "description": "Joint variability of X and Y",
        "examples": [
          "Cov(X,Y)"
        ]
      }
    },
    {
      "id": "correlation",
      "metadata": {
        "type": "concept",
        "description": "Scaled covariance in [-1,1]",
        "examples": [
          "Pearson r"
        ]
      }
    },
    {
      "id": "regression",
      "metadata": {
        "type": "concept",
        "description": "Model relationships",
        "examples": [
          "linear",
          "logistic"
        ]
      }
    },
    {
      "id": "linear_regression",
      "metadata": {
        "type": "concept",
        "description": "y=β0+β1x+ε",
        "examples": [
          "OLS"
        ]
      }
    },
    {
      "id": "logistic_regression",
      "metadata": {
        "type": "concept",
        "description": "P(Y=1|x)=σ(β·x)",
        "examples": [
          "classification"
        ]
      }
    },
    {
      "id": "hypothesis_test",
      "metadata": {
        "type": "concept",
        "description": "Assess evidence vs null hypothesis",
        "examples": [
          "t-test",
          "χ²"
        ]
      }
    },
    {
      "id": "null_hypothesis",
      "metadata": {
        "type": "concept",
        "description": "Baseline assumption",
        "examples": [
          "μ=μ0"
        ]
      }
    },
    {
      "id": "alternative_hypothesis",
      "metadata": {
        "type": "concept",
        "description": "Competing claim",
        "examples": [
          "μ≠μ0"
        ]
      }
    },
    {
      "id": "p_value",
      "metadata": {
        "type": "concept",
        "description": "Extremeness under H0",
        "examples": [
          "p=0.03"
        ]
      }
    },
    {
      "id": "type_I_error",
      "metadata": {
        "type": "concept",
        "description": "False positive rate α",
        "examples": [
          "reject true H0"
        ]
      }
    },
    {
      "id": "type_II_error",
      "metadata": {
        "type": "concept",
        "description": "False negative rate β",
        "examples": [
          "fail to reject false H0"
        ]
      }
    },
    {
      "id": "power",
      "metadata": {
        "type": "concept",
        "description": "1-β: probability to detect effect",
        "examples": [
          "power analysis"
        ]
      }
    },
    {
      "id": "effect_size",
      "metadata": {
        "type": "concept",
        "description": "Magnitude of difference",
        "examples": [
          "Cohen's d",
          "odds ratio"
        ]
      }
    },
    {
      "id": "confidence_interval",
      "metadata": {
        "type": "concept",
        "description": "Range likely to contain parameter",
        "examples": [
          "95% CI"
        ]
      }
    },
    {
      "id": "central_limit_theorem",
      "metadata": {
        "type": "concept",
        "description": "Sampling mean tends to normal",
        "examples": [
          "large n"
        ]
      }
    },
    {
      "id": "law_of_large_numbers",
      "metadata": {
        "type": "concept",
        "description": "Sample mean→E[X]",
        "examples": [
          "n→∞"
        ]
      }
    },
    {
      "id": "likelihood",
      "metadata": {
        "type": "concept",
        "description": "Function of parameters given data",
        "examples": [
          "L(θ|x)"
        ]
      }
    },
    {
      "id": "maximum_likelihood_estimation",
      "metadata": {
        "type": "concept",
        "description": "Choose θ maximizing L",
        "examples": [
          "MLE"
        ]
      }
    },
    {
      "id": "bayes_theorem",
      "metadata": {
        "type": "equation",
        "description": "P(A|B)=P(B|A)P(A)/P(B)",
        "examples": [
          "posterior update"
        ]
      }
    },
    {
      "id": "prior",
      "metadata": {
        "type": "concept",
        "description": "Belief before data",
        "examples": [
          "Beta prior"
        ]
      }
    },
    {
      "id": "posterior",
      "metadata": {
        "type": "concept",
        "description": "Updated belief after data",
        "examples": [
          "Beta posterior"
        ]
      }
    },
    {
      "id": "conditional_probability",
      "metadata": {
        "type": "concept",
        "description": "P(A|B)",
        "examples": [
          "disease testing"
        ]
      }
    },
    {
      "id": "t_test",
      "metadata": {
        "type": "concept",
        "description": "Compare means via t",
        "examples": [
          "one/two-sample"
        ]
      }
    },
    {
      "id": "z_test",
      "metadata": {
        "type": "concept",
        "description": "Test using normal z",
        "examples": [
          "known σ"
        ]
      }
    },
    {
      "id": "anova",
      "metadata": {
        "type": "concept",
        "description": "Compare >2 means",
        "examples": [
          "F-test"
        ]
      }
    },
    {
      "id": "nonparametric_tests",
      "metadata": {
        "type": "concept",
        "description": "Distribution-free tests",
        "examples": [
          "Mann-Whitney",
          "Kruskal-Wallis"
        ]
      }
    },
    {
      "id": "bootstrap",
      "metadata": {
        "type": "concept",
        "description": "Resampling with replacement",
        "examples": [
          "CI by bootstrap"
        ]
      }
    },
    {
      "id": "cross_validation",
      "metadata": {
        "type": "concept",
        "description": "Estimate generalization by CV",
        "examples": [
          "k-fold"
        ]
      }
